{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "phase-2/backend/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "phase-2/frontend/$1"
    }
  ],
  "builds": [
    {
      "src": "phase-2/frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "phase-2/backend/main.py",
      "use": "@vercel/python"
    }
  ]
}
